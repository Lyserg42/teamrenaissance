<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>CardServlet</servlet-name>
    <servlet-class>fr.teamrenaissance.dar.servlets.CardServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>UserServlet</servlet-name>
    <servlet-class>fr.teamrenaissance.dar.servlets.UserServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>LoanServlet</servlet-name>
    <servlet-class>fr.teamrenaissance.dar.servlets.LoanServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>TournamentServlet</servlet-name>
    <servlet-class>fr.teamrenaissance.dar.servlets.TournamentServlet</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>CardServlet</servlet-name>
    <url-pattern>/card</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/user</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>LoanServlet</servlet-name>
    <url-pattern>/loan</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>TournamentServlet</servlet-name>
    <url-pattern>/tournament</url-pattern>
  </servlet-mapping>

</web-app>
